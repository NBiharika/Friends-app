<% provide(:friend_first_name, 'Freind Details') %>

<div class="jumbotron">
  <h1 class="display-4">

    <% @user.each do |user| %>
      <h1><%= user.first_name %> <%= user.last_name %></h2>
      <p class="lead">
    <%= user.email %>  | 
    <%= user.phone %>  |  
    <%= user.twitter %>

  </p>
    <% end %>
    



  <%= link_to 'Back', friends_path, class:"btn btn-dark" %>
  <%= link_to 'Delete', @friend, method: :delete, class: "btn btn-danger" %>
</div>
